	keywords = {Anxiety Disorders: physiopathology;Mice;Mice, Transgenic;Humans;Disease Models, Animal;Mice, Transgenic: physiology;F30bib;NIMH_MDPhD_conference;Depression: physiopathology;Animals;Anxiety Disorders;Depression},
	tags = {NIMH\_MDPhD\_conference;F30bib},
