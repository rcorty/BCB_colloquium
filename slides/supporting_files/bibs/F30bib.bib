	url = {http://dx.doi.org/10.1038/nrn2132},
	keywords = {Neurons: physiology;Brain;Mental Disorders: pathology;Mental Disorders;Brain: pathology;Mental Disorders: physiopathology;Mental Disorders: genetics;Humans;Gene Expression Regulation: physiology;Neurons;Models, Biological;NIMH_MDPhD_conference;F30bib;Animals;Gene Expression Regulation;Epigenesis, Genetic: physiology;Epigenesis, Genetic},
